package br.com.universidade.modelo;



import javax.swing.JOptionPane;

public class Aluno {
	
	private int rm;
	private String nome;
	private String email;
	
	public int getRm() {
		return rm;
	}
	public void setRm(int rm) {
		this.rm = rm;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	
	

	}



				
		
		
		
		
/*
 * Design Patterns ( Melhores praticas para projetos com Orientacao Objeto;
 * DTO (Data Transfer Object) - melhores praticas para os modelos (Beans/JavaBeans/TO/Model)
 * 1ª Sugestão : Todos atributos devem ser privados.
 * 2ª Sugestão : Cada atribuido dever ter o seu netodo getter ( output) e setter ( input)
 * 3º Sugestão :
 * 
 * Aplicar DTO tem que esta dentro da class e abaixo do atributo. 
 */	
	


