package br.com.lojauati.modelo;

public class Formula1 {
	
	/*
	 * sinal "-" no grafico padrão visibilidade UML (astah) representa o private no java, podendo ser
	 * alterada (public....)
	 * private segurança do produto
	 * Sintaxe para criação de metodos do java
	 * - <modificador> <tipo do retorno> <nome do metodo> (<tipo do param>
	 */
	
	private String escuderia;
	private float valor;
	private int rpm;
	
	public void preencherEscuderia(String parametro) {
		escuderia = parametro.toUpperCase();
		
	}

}
